import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here/
		Scanner ip=new Scanner(System.in);
	    int t=ip.nextInt();
	    while(t-->0){
	        int a=ip.nextInt();
	        int b=ip.nextInt();
	        int c=ip.nextInt();
	        int res = b+2*c;
	        if(res <= a){
	            System.out.println("YES");
	        }
	        else{
	            System.out.println("NO");
	        }
	    }

	}
}
