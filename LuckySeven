import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner ip = new Scanner(System.in);
        String S = ip.nextLine();
        char luckyLetter = S.charAt(6);
        System.out.println(luckyLetter);

	}
}
