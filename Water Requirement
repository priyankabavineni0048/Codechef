import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner ip=new Scanner(System.in);
        int t=ip.nextInt();
        while(t-->0){
            int n=ip.nextInt();
            System.out.println(n*2);
        }

	}
}
