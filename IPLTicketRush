import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner ip = new Scanner(System.in);
        int T = ip.nextInt();
        for (int i = 0; i < T; i++) {
            int N = ip.nextInt();
            int M = ip.nextInt();
            int studentsWithoutTickets = Math.max(0, N - M);
            System.out.println(studentsWithoutTickets);
        }
	}
}
