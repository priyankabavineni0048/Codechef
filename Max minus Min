import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner ip = new Scanner(System.in);
        int T = ip.nextInt(); 
        for (int i = 0; i < T; i++) {
            int A = ip.nextInt();
            int B = ip.nextInt();
            int C = ip.nextInt();
            int max = C;
            int min = A;
            int result = max - min;
            System.out.println(result);
        }


	}
}
