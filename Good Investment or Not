import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner ip = new Scanner(System.in);
        int T = ip.nextInt();
        for (int i = 0; i < T; i++) {
            int X = ip.nextInt();
            int Y = ip.nextInt();
            if (X >= 2 * Y) {
                System.out.println("YES");
            } else {
                System.out.println("NO");
            }
        }

	}
}
