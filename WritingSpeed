import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner ip = new Scanner(System.in);
        int X = ip.nextInt();
        int totalPages = 5;
        int totalTime = 60;
        int requiredTime = X * totalPages;
        if (requiredTime <= totalTime) {
            System.out.println("YES");
        } else {
            System.out.println("NO");
        }
        

	}
}
