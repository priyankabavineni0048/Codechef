import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner ip = new Scanner(System.in);
        int T = ip.nextInt();
        for (int t = 0; t < T; t++) {
            int N = ip.nextInt();
            int M = ip.nextInt();
            int totalWords = N * M;
            System.out.println(totalWords);
        }

	}
}
