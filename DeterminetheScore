import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
	    Scanner ip = new Scanner(System.in);
        int T = ip.nextInt();
        for (int i = 0; i < T; i++) {
            int X = ip.nextInt();
            int N = ip.nextInt();
            int y = X / 10;
            int score = y * N;
            System.out.println(score);
        }

	}
}
