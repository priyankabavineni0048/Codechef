import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner ip=new Scanner(System.in);
	    int T = ip.nextInt();  
        for (int i = 0; i < T; i++) {
            int X = ip.nextInt(); 
            int Y = ip.nextInt();  
            int ans = Math.max(0, X - Y);
            System.out.println(ans);
        }
	}
}
