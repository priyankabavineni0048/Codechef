import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
        Scanner scanner = new Scanner(System.in);
        int N = scanner.nextInt();
        
        if (N >= 16 && N <= 18) {
            System.out.println("ADVITIYA");
        } else {
            System.out.println("WAITING FOR ADVITIYA");
        }
        
        scanner.close();
	}
}
