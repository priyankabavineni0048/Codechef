import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner ip = new Scanner(System.in);
        int N = ip.nextInt(); 
		int x = 100;
        int y = x - N; 
        System.out.println(y);

	}
}
