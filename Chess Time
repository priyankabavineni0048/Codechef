import java.util.*;
import java.lang.*;
import java.io.*;

class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		// your code goes here
		Scanner ip = new Scanner(System.in);
        int T = ip.nextInt(); 
        
        for (int i = 0; i < T; i++) {
            int N = ip.nextInt(); 
            int res = (N * 60) / 20; 
            
            System.out.println(res);
        }
        

	}
}
